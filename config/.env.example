# Tradovate API Credentials
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# Environment: "demo" or "live"
TRADOVATE_ENV=demo

# Your Tradovate login credentials
TRADOVATE_USERNAME=your_username
TRADOVATE_PASSWORD=your_password

# Optional: OAuth credentials (if you have them)
# TRADOVATE_CID=your_client_id
# TRADOVATE_SECRET=your_client_secret

# Optional: Device ID (auto-generated if not provided)
# TRADOVATE_DEVICE_ID=your_device_id
